type Query {
	firstQuery : String
	secondQuery : String
	fullName(sampleRequest : SampleRequest) : String
	student(id : Long) : StudentResponse
}

input SampleRequest {
	firstName : String!
	lastName : String
}

type StudentResponse {
	id : Long
	firstName : String
	lastName : String
	email : String
	addresses : [AddressResponse]
	fullName : String
}


type AddressResponse {
	id : Long
	city: String
	street: String
}

